{
	"name": "Loop",
	"version": "0.1.0",
	"author": [
		"Marc Vorreiter"
	],
	"url": "https://github.com/oncampus/loop_extension_Loop",
	"descriptionmsg": "loop-desc",
	"license-name": "GPL-2.0+",
	"type": "other",
	"callback": "Loop::onExtensionLoad",
	"AutoloadClasses": {
		"Loop": "includes/Loop.php",
		"LoopHooks": "includes/Loop.hooks.php"
	},
	"Hooks": {
		"ParserBeforeStrip": [
			"LoopHooks::onParserBeforeStrip"
		]	
	},
	"namespaces": [
		{
			"id": 3100,
			"constant": "NS_LOOP_TOC",
			"name": "TOC",
			"protection": "loop-toc-edit"
		},
		{
			"id": 3101,
			"constant": "NS_LOOP_TOC_TALK",
			"name": "TOC_talk"
		}
	],	
	"GroupPermissions": {
		"*": {
			"loop-toc-edit": false
		},
		"user": {
			"loop-toc-edit": false
		},
		"autoconfirmed": {
			"loop-toc-edit": false
		},
		"bot": {
			"loop-toc-edit": true
		},
		"bureaucrat": {
			"loop-toc-edit": true
		},		
		"sysop": {
			"loop-toc-edit": true
		}
	},	
	"AvailableRights": [
		"loop-toc-edit"
	],
	"MessagesDirs": {
		"Loop": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "Loop"
	},
	"manifest_version": 1
}











